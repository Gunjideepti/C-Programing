#include<stdio.h>
int main()
{
    int a;
    printf("enter percent ");
    scanf("%d",&a);
    if(a>90&&a<=100)
    {
        printf("Distinction");
    }
    if(a>80&&a<=90)
    {
    printf("First Class");
    }
    if(a>70&&a<=80)
    {
    printf("second Class");
    }
    if(a>60&&a<=70)
    {
    printf("Third Class");
    }
    if(a<=60)
    {
    printf("Fail");
    }
        return 0;
}
